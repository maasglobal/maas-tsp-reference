{
  "name": "bookings-create",
  "runtime": "nodejs4.3",
  "description": "Creates a new Booking for the TSP in booked state.",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": ["Content-Type", "X-Api-Key"]
    }
  },
  "endpoints": [
    {
      "path": "/bookings",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "application/json": {
        "leg": "$input.json('$.leg')",
        "customer": "$input.json('$.customer')",
        "meta": "$input.json('$.meta')"
      },
      "responses": {
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        },
        "500": {
          "statusCode": "500",
          "selectionPattern": "Error:",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          },
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
